<script>
	previewClean();
	$(window).on('load', function(){
		measurements();
	});
	$(window).resize(function(){
		measurements();
	});
	function measurements(){
		$('#pageWidth').text(function(){
			return window.innerWidth;
		})
		$('.grid div').each(function(){
			$(this).find('.modWidth__left').text(function(){
				return $(this).parents('div[class*="grid__unit"]').css('padding-left');
			});
			$(this).find('.modWidth__module').text(function(){
				return $(this).parents('div[class*="grid__unit"]').width();
			});
			$(this).find('.modWidth__right').text(function(){
				return $(this).parents('div[class*="grid__unit"]').css('padding-right');
			});
		});
	}
</script>